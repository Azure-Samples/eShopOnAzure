{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost",
    "EventBus": "amqp://localhost"
  },
  "Identity": {
    "Audience": "https://localhost:7298/",
