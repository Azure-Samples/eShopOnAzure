  },
  "EventBus": {
    "SubscriptionClientName": "Basket"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "<tenant id>",
    "ClientId": "<client id>",
    "Scopes": "basket"
  }
}